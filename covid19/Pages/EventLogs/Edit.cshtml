@page
@model covid19.Pages.EventLogs.EditModel

@{
    //Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

<h4>EventLog</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EventLog.ID" />
            <div class="form-group">
                <label asp-for="EventLog.LocationID" class="control-label"></label>
                <select asp-for="EventLog.LocationID" class="form-control" asp-items="ViewBag.LocationID"></select>
                <span asp-validation-for="EventLog.LocationID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EventLog.StudentID" class="control-label"></label>
                <select asp-for="EventLog.StudentID" class="form-control" asp-items="ViewBag.StudentID"></select>
                <span asp-validation-for="EventLog.StudentID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EventLog.TimeStamp" class="control-label"></label>
                <input asp-for="EventLog.TimeStamp" class="form-control" />
                <span asp-validation-for="EventLog.TimeStamp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
</body>
</html>
